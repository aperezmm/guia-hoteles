{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "Curso coursera",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss \" \" npm run dev \" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin register.html -d dist --htmlmin -o dist/register.html && usemin tarifa.html -d dist --htmlmin -o dist/tarifa.html",
    "build": "npm run clean && npm run scss && npm run imagemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aperezmm/guia-hoteles.git"
  },
  "author": "Alejandro Perez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aperezmm/guia-hoteles/issues"
  },
  "homepage": "https://github.com/aperezmm/guia-hoteles#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "copyfiles": "^2.3.0",
    "cssmin": "^0.4.3",
    "grunt": "^1.3.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.0.0",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^6.0.0",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "gifsicle": "^5.1.0",
    "grunt-sass": "^3.1.0",
    "imagemin": "^7.0.1",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "uglify-js": "^3.10.0"
  }
}
